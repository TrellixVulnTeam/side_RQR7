Train解壓縮
tar -xvf ILSVRC2012_img_train.tar -C train
tar -xvf ILSVRC2012_img_train_t3.tar -C train

for x in `ls train/*tar`; do fn=train/`basename $x .tar`; mkdir $fn; tar -xvf $x -C $fn; rm -f $fn.tar; done
cd train; find . -name "*.tar" | while read NAME ; do mkdir -p "${NAME%.tar}"; tar -xvf "${NAME}" -C "${NAME%.tar}"; rm -f "${NAME}"; done

Validation
tar -xvf ILSVRC2012_img_val.tar -C val
mv valprep.sh val && cd val && bash valprep.sh